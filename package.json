{
  "name": "@dfinity/feedback",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "setup": "npm install && dfx canister create backend && dfx generate backend && dfx deploy",
    "start": "run-p frontend backend",
    "frontend": "vite --port 3000",
    "backend": "mo-dev --generate --deploy -y",
    "build:frontend": "vite build",
    "build": "npm run build:frontend",
    "test": "run-s test:mo test:ts",
    "test:mo": "mo-test",
    "test:ts": "vitest run",
    "deploy": "npm run test && dfx deploy",
    "deploy:prod": "npm run test && npm run build && netlify deploy --dir=dist --prod",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "sources": "mops sources",
    "postinstall": "mops install"
  },
  "dependencies": {
    "@dfinity/agent": "^0.15.4",
    "@dfinity/candid": "^0.15.4",
    "@dfinity/principal": "^0.15.4",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0",
    "twin.macro": "^3.1.0",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.2.0",
    "@emotion/react": "^11.10.6",
    "@netlify/functions": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "ic-mops": "^0.8.7",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "mo-dev": "^0.5.4",
    "netlify": "^13.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "tailwindcss": "^3.2.7",
    "vite": "^4.2.0",
    "vite-imagetools": "^4.0.18",
    "vitest": "^0.29.3",
    "web-vitals": "^3.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ]
