    "@dfinity/principal": "^0.15.4",
    "@emotion/styled": "^11.10.6",
    "@tippyjs/react": "^4.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.7",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.6",
    "react-router-dom": "^6.9.0",
